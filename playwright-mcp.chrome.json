{
  "browser": {
    "browserName": "chromium",
    "isolated": true,
    "launchOptions": {
      "channel": "chrome",
      "headless": true
    },
    "contextOptions": {
      "viewport": { "width": 1280, "height": 800 }
    }
  },
  "server": {
    "host": "127.0.0.1",
    "port": 33556
  },
  "capabilities": ["tabs", "pdf", "vision"],
  "outputDir": "./.mcp-output",
  "imageResponses": "allow"
}
